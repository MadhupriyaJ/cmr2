@section Scripts{
    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <script type="text/javascript" src="@Url.Content("~/")assets/global/plugins/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>
    <script type="text/javascript" src="@Url.Content("~")assets/global/plugins/jquery-validation/js/jquery.validate.min.js"></script>
    <script type="text/javascript" src="@Url.Content("~")assets/global/plugins/jquery-validation/js/additional-methods.min.js"></script>
    <script type="text/javascript" src="@Url.Content("~")assets/global/plugins/bootstrap-wizard/jquery.bootstrap.wizard.min.js"></script>
    <script type="text/javascript" src="@Url.Content("~")assets/global/plugins/bootstrap-fileinput/bootstrap-fileinput.js"></script>
    <!-- END PAGE LEVEL PLUGINS -->
    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <script type="text/javascript" src="@Url.Content("~")assets/global/plugins/select2/select2.min.js"></script>
    <!-- END PAGE LEVEL PLUGINS -->
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    <script src="@Url.Content("~/")assets/Programmer/Scripts/Person/formwizardperson.js"></script>
    <script src="@Url.Content("~/")assets/global/plugins/bootbox/bootbox.min.js" type="text/javascript"></script>

    <!-- END PAGE LEVEL SCRIPTS -->
    <script>
    jQuery(document).ready(function () {
        FormWizard.init();
        $("#AddCompany").hide();

        $('#AddCompany').live('click', function () {
            $('#large').modal('show');
            $('#btnmodalsave').live('click', function () {
                $('#large').modal('hide');
            });
        });

        $("#companyislisted").live('click', function () {
            if ($("#companyislisted").prop('checked') == true) {
                $("#AddCompany").show();
                $("#Company").prop('disabled', true);
            }
            else {
                $("#AddCompany").hide();
                $("#Company").prop('disabled', false);
            }

        });

        $("#Country").change(function () {
            var postdata = { ID: $("#Country option:selected").val() }
            $.ajax({
                type: "GET",
                url: '@Url.Content("~")StateManagement/LoadState',
                data: postdata,
                success: function (data) {
                    $("#State").empty();
                    $("#State").append("<option value=\"\">Select</option>");
                    $.each(data, function (key, val) {
                        $("#State").append("<option value=" + val.Id + ">" + val.Name + "</option>");
                    });
                },
                traditional: true
            });
        });

        $("#State").change(function () {

            var postdata = { ID: $("#State option:selected").val() }
            $.ajax({
                type: "GET",
                url: '@Url.Content("~")CityManagement/LoadCity',
                data: postdata,
                success: function (data) {
                    $("#City").empty();
                    $("#City").append("<option value=\"\">Select</option>");
                    $.each(data, function (key, val) {
                        $("#City").append("<option value=" + val.Id + ">" + val.Name + "</option>");
                    });
                },
                traditional: true
            });
        });


        $("#MCountry").change(function () {
            var postdata = { ID: $("#MCountry option:selected").val() }
            $.ajax({
                type: "GET",
                url: '@Url.Content("~")StateManagement/LoadState',
                data: postdata,
                success: function (data) {
                    $("#MState").empty();
                    $("#MState").append("<option value=\"\">Select</option>");
                    $.each(data, function (key, val) {
                        $("#MState").append("<option value=" + val.Id + ">" + val.Name + "</option>");
                    });
                },
                traditional: true
            });
        });

        $("#MState").change(function () {

            var postdata = { ID: $("#MState option:selected").val() }
            $.ajax({
                type: "GET",
                url: '@Url.Content("~")CityManagement/LoadCity',
                data: postdata,
                success: function (data) {
                    $("#MCity").empty();
                    $("#MCity").append("<option value=\"\">Select</option>");
                    $.each(data, function (key, val) {
                        $("#MCity").append("<option value=" + val.Id + ">" + val.Name + "</option>");
                    });
                },
                traditional: true
            });
        });



    });
    </script>
}

