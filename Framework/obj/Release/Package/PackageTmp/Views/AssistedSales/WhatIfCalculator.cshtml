                                <div class="form-group">
                                    <label class="control-label col-sm-3">Type Of Finance</label>
                                    <div class="col-md-4">
                                        <select class="form-control" data-ng-model="selectedLiablity.FinanceType" placeholder="Select"
                                                data-ng-options="finance as finance.Type for finance in lstFinanceType" required>
                                            <option value="">Select</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-sm-3">Finance Amount</label>
                                    <div class="col-md-4">
                                        <input type="text" class="form-control currenciesOnly" data-ng-model="selectedLiablity.FinanceAmount" style="text-align:right" required />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-sm-3">Finance Start Date</label>
                                    <div class="col-md-4">
                                        <input type="text" class="form-control date-picker" id="txtFromDate" placeholder="MM/DD/YYYY" data-ng-model="selectedLiablity.StartDate" required />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-sm-3">Finance End Date</label>
                                    <div class="col-md-4">
                                        <input type="text" class="form-control date-picker" id="txtToDate" placeholder="MM/DD/YYYY" data-ng-model="selectedLiablity.EndDate" required />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-sm-3">Monthly Installment</label>
                                    <div class="col-md-4">
                                        <input type="text" class="form-control currenciesOnly" data-ng-model="selectedLiablity.MonthlyInstallment" style="text-align:right" required />
                                    </div>
                                </div>
                            </div>
                            <div class="form-actions">
                                <div class="row">
                                    <div class="col-md-offset-3 col-md-9">
                                        <input id="btnSubmit" type="submit" value="Save" class="btn green" />
                                        <input type="button" value="Cancel" data-ng-click="cancelModal()" class="btn btn-default" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </modal>

            </div>
        </div>
    </div>
</div>

