    var userassigned = dc.Employees.Where(a => a.OrgId==Login.OrganizationId && a.MarkAsDelete==false && a.UserId == Login.Id).Select(a => a.Id).FirstOrDefault();
    var list = dc.PersonLeads.AsEnumerable()
                           .Where(a =>
                               a.OrgId == Login.OrganizationId
                               && a.Assignedto == userassigned
                               && a.MarkAsDelete == false).Select(a => new { a.ExpectedValue ,a.DealStageNameId});
    var won = list.Where(a => a.DealStageNameId == new Guid("8fc13a30-255d-41dd-8385-ded2987cb93c")).Select(a=>a.ExpectedValue).Sum();
    var lost = list.Where(a => a.DealStageNameId == new Guid("b4a42d7c-0cac-42cd-a464-ef1a504cb5b8")).Select(a => a.ExpectedValue).Sum();
    var dropped = list.Where(a => a.DealStageNameId == new Guid("1adbe8a1-5b00-4a01-830a-bad79927707e")).Select(a => a.ExpectedValue).Sum();
    var ongoing = list.Where(a => a.DealStageNameId != new Guid("b4a42d7c-0cac-42cd-a464-ef1a504cb5b8")
                                  && a.DealStageNameId != new Guid("8fc13a30-255d-41dd-8385-ded2987cb93c")
                                  && a.DealStageNameId != new Guid("1adbe8a1-5b00-4a01-830a-bad79927707e")).Select(a => a.ExpectedValue).Sum();
    var contactcnt = dc.People.Where(a => a.OrgId == Login.OrganizationId && a.MarkAsDelete == false).Count();
    var taskcnt = dc.CompanyCustomerAppoinment.Where(a => a.MarkAsDelete == false && a.OrgId == Login.OrganizationId && a.appStatus == false).Count();
    var campaignCount = dc.Campaigns.Where(a => a.OrgId == Login.OrganizationId && a.MarkAsDelete == false).Count();
