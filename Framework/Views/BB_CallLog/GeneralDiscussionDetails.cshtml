@model PagedList.IPagedList<FramWork.Models.ABB_CallLogGeneralDiscussion>

@{
    Layout = null;
}
<div class="row">
    <div class="col-md-10" style="margin-left:60px;">
        <table class="table table-striped table-bordered table-advance table-hover">
            <tr><td align="center"><h4>Call Log Subject: @Session["CallLogSubject"]</h4></td></tr>
        </table>


        @foreach (var item in Model.Select((x, i) => new { Data = x, Index = i }))
        {
            <div class="row">
                <div class="col-md-12">
                    <h4>Title:  @item.Data.Title &nbsp;&nbsp;&nbsp;&nbsp;<a href="#" id="EditGeneralDisc" value="@item.Data.Id" class="btn btn-circle btn-sm red"> Edit <i class="fa fa-edit"></i>
</a></h4>
    <table class="table table-striped table-bordered table-advance table-hover">
        <tr>
            <td>
                Title
            </td>

            <td>
                @item.Data.Title
            </td>
        </tr>
        <tr>
            <td>
                General Discussion
            </td>

            <td>
                @item.Data.GeneralDiscussion
            </td>
        </tr>
        <tr>
            <td>
                Create Date
            </td>

            <td>
                @item.Data.CreateDate.ToShortDateString()
            </td>
        </tr>
        <tr>
            <td>
                Modified Date
            </td>

            <td>
                @item.Data.ModifiedDate.ToShortDateString()
            </td>
        </tr>
    </table>
</div>
            </div>
            <br />


        }



    </div>
        </div>
