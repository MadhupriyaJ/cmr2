@using Core;
@using FramWork.HelloService;
@{
    ViewBag.Title = "Lead2Deal | Case Item";
    Layout = "~/Views/Shared/_LayoutServices.cshtml";
    HelloServiceClient client = new HelloServiceClient("BasicHttpBinding_IHelloService");
    var dealstage = client.GetCaseManagmentStageList(Login.OrganizationId);
    var stagewidths = 100 / dealstage.Count();
    var caseids = ViewBag.PatientId.ToString();
    var details = new CaseManagment();
    if (!String.IsNullOrEmpty(caseids)) { 
         details=client.GetCaseDetails(Login.OrganizationId,new Guid(caseids));
    }
}
