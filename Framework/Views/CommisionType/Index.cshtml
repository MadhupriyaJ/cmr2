@model IEnumerable<FramWork.Models.Commision_CommisionType>

@{
    ViewBag.Title = "Lead2Deal | Comission Type List";
    Layout = "~/Views/Shared/_LayoutHome.cshtml";
}
<ul class="page-breadcrumb breadcrumb">
    <li>
        <a href="@Url.Content("~/")Home/Index">Home</a>
        <i class="fa fa-circle"></i>
    </li>
    <li>
        <a href="#">Sales</a>
        <i class="fa fa-circle"></i>
    </li>
    <li>
        <a href="#">Goal Config</a>
        <i class="fa fa-circle"></i>
    </li>
    <li>
        <a href="#">Comission Type List</a>
    </li>

</ul>

<div class="portlet light bordered">
    <div class="portlet-title">
        <div class="caption font-yellow-crusta">
            <i class="icon-share font-yellow-crusta"></i>
            <span class="caption-subject bold uppercase"> Comission Type List</span>
            <span class="caption-helper"></span>
        </div>
        <div class="actions">
            <a class="btn btn-primary" href="@Url.Content("~")/CommisionType/Create">
                <i class="icon-plus"></i> Add New
            </a>
            <a href="#" class="btn btn-circle btn-default btn-icon-only fullscreen green" style="color:white;" data-original-title="" title=""></a>
        </div>
    </div>
    <div class="portlet-body">
        <table class="table table-striped table-bordered table-advance table-hover">
            <tr>
               
                <th>
                    @Html.DisplayNameFor(model => model.CommisionType)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CreateDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ModifiedDate)
                </th>
                <th>Actions</th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                  
                    <td>
                        @Html.DisplayFor(modelItem => item.CommisionType)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CreateDate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ModifiedDate)
                    </td>
                  
                  
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                    </td>
                </tr>
            }

        </table>
    </div>
</div>


